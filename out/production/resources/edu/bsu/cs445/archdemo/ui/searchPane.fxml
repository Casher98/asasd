<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<fx:root maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="594.0" minWidth="800.0" prefHeight="594.0" prefWidth="652.0" type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
    <Label text="Enter Search Below." BorderPane.alignment="CENTER" />
    <HBox fx:id="searchHBox">
        <TextField fx:id="searchField" onAction="#search" />
        <Button fx:id="searchButton" onAction="#search" text="Search" />
    </HBox>
    <HBox>
        <Label text="Last search yielded " />
        <Label fx:id="resultCount" text="0" />
        <Label text=" results" />
    </HBox>
   <ScrollPane>
      <content>
          <VBox fx:id="resultBox" />
      </content>
   </ScrollPane>
</fx:root>
